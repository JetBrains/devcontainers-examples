{
  //This example demonstrates how to install plugins in a Dev Container using the “customization” option for JetBrains IDE.
  //You can find the pluginID in the JetBrains Marketplace
  //(https://plugins.jetbrains.com/plugin/7808-terraform-and-hcl?noRedirect=true)
  //under the “Additional Information” section.
  "name": "minimal_ubuntu",
  "image": "ubuntu:latest",
  "customizations": {
    "jetbrains": {
      "plugins": [
        "org.intellij.plugins.hcl"
      ],
    },
  }
}